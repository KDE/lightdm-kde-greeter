#: PrimaryScreen.qml:627
#, kde-format
msgid "Desktop session"
msgstr "הפעלת שולחן עבודה"

#: PrimaryScreen.qml:651
#, kde-format
msgid "Suspend"
msgstr "השהיה"

#: PrimaryScreen.qml:662
#, kde-format
msgid "Hibernate"
msgstr "הרדמה"

#: PrimaryScreen.qml:674
#, kde-format
msgid "Restart"
msgstr "הפעלה מחדש"

#: PrimaryScreen.qml:685
#, kde-format
msgid "Shutdown"
msgstr "כיבוי"

