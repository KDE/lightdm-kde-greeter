#: config.qml:37
#, kde-format
msgid "Background image:"
msgstr "Atzeko planoko irudia:"

#: NetworkWidget.qml:24
#, kde-format
msgid "The connection will be active only on the login screen."
msgstr "Konexioa saio-sarrerako pantailan bakarrik egongo da aktibo."

#: NetworkWidget.qml:61
#, kde-format
msgid "Disconnect from %1 ?"
msgstr "%1(e)tik deskonektatuta?"

#: NetworkWidget.qml:64
#, kde-format
msgid "Abort connecting to %1 ?"
msgstr "%1(e)rekiko konexioa bertan behera utzi?"

#: NetworkWidget.qml:68
#, kde-format
msgid "Connect to %1 ?"
msgstr "%1(e)ra konektatu?"

#: NetworkWidget.qml:72
#, kde-format
msgid "Connect to an insecure access point %1 ?"
msgstr "%1 sarbide-puntu ez-seguru batera konektatu?"

#: NetworkWidget.qml:77
#, kde-format
msgid "Enter the password to connect to %1"
msgstr "Sartu %1(e)ra konektatzeko pasahitza"

#: NetworkWidget.qml:82
#, kde-format
msgid "Enter login and password to connect to %1"
msgstr "Sartu %1(e)ra konektatzeko saio-sarrera eta pasahitza"

#: NetworkWidget.qml:86 NetworkWidget.qml:92
#, kde-format
msgid "%1: Connection error, try again."
msgstr "%1: Konexio errorea, berriz saiatu."

#: NetworkWidget.qml:97
#, kde-format
msgid "Provide the secrets to connect to %1:"
msgstr "Hornitu %1(e)era konektatzeko sekretuak:"

#: NetworkWidget.qml:101
#, kde-format
msgid "The security protocol is not supported for %1"
msgstr "Segurtasun protokoloak ez dauka %1(e)ren euskarririk"

#: NetworkWidget.qml:104 NetworkWidget.qml:108
#, kde-format
msgid "Failed to connect to %1"
msgstr "%1(e)ra konektatzea huts egin du"

#: NetworkWidget.qml:112
#, kde-format
msgid "Error: Can't find access point %1"
msgstr "Errorea: Ezin du %1 sarbide-puntua aurkitu"

#: NetworkWidget.qml:440
#, kde-format
msgid "Login"
msgstr "Saio-sarrera"

#: NetworkWidget.qml:455
#, kde-format
msgid "Password"
msgstr "Pasahitza"

#: NetworkWidget.qml:507
#, kde-format
msgid "Disable wireless"
msgstr "Ezgaitu haririk gabekoa"

#: NetworkWidget.qml:507
#, kde-format
msgid "Enable wireless"
msgstr "Gaitu haririk gabekoa"

#: NetworkWidget.qml:508 NetworkWidget.qml:549
#, kde-format
msgid "Action prohibited"
msgstr "Debekatutako ekintza"

#: NetworkWidget.qml:548
#, kde-format
msgid "Disable networking"
msgstr "Ezgaitu sarea"

#: NetworkWidget.qml:548
#, kde-format
msgid "Enable networking"
msgstr "Gaitu sarea"

#: OtherScreen.qml:27
#, kde-format
msgid "Press %1 to change the primary screen"
msgstr "%1 sakatu pantaila nagusia aldatzeko"

#: PrimaryScreen.qml:90
#, kde-format
msgid "Login failed"
msgstr "Saio-sarrerak huts egin du"

#: PrimaryScreen.qml:501
#, kde-format
msgid "Caps Lock is on"
msgstr "«Maius.Giltz.» piztuta dago"

#: PrimaryScreen.qml:504
#, kde-format
msgid "Num Lock is on"
msgstr "«Zk.Gitz.» piztuta dago"

#: PrimaryScreen.qml:507
#, kde-format
msgid "Caps Lock and Num Lock is on"
msgstr "«Maius.Giltz.» eta «Zk.Giltz.» piztuta daude"

#: PrimaryScreen.qml:520
#, kde-format
msgid "Log in"
msgstr "Saioan sartu"

#: PrimaryScreen.qml:539
#, kde-format
msgid "Log in as another user"
msgstr "Saioan sartu beste erabiltzaile bat bezala"

#: PrimaryScreen.qml:698
#, kde-format
msgid "Keyboard layout"
msgstr "Teklatuaren antolaera"

#: PrimaryScreen.qml:743
#, kde-format
msgid "Desktop session"
msgstr "Mahaigaineko saioa"

#: PrimaryScreen.qml:770
#, kde-format
msgid "Suspend"
msgstr "Eseki"

#: PrimaryScreen.qml:782
#, kde-format
msgid "Hibernate"
msgstr "Hibernatu"

#: PrimaryScreen.qml:795
#, kde-format
msgid "Restart"
msgstr "Berrabiarazi"

#: PrimaryScreen.qml:807
#, kde-format
msgid "Shutdown"
msgstr "Itzali"

#: PrimaryScreen.qml:837
#, kde-format
msgctxt "Button to show/hide virtual keyboard"
msgid "Virtual Keyboard"
msgstr "Alegiazko teklatua"
